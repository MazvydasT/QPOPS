{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/default.aspx",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/default.aspx",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js",
          "/favicon.ico",
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "urls": [
          "https://fonts.g**/**"
        ]
      }
    }
  ]
}